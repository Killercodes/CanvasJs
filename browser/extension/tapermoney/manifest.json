{
  "manifest_version": 2,
  "name": "Tampermonkey-like Extension",
  "version": "1.0",
  "description": "A user script manager extension",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage",
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Tampermonkey-like Extension",
    "default_popup": "popup.html"
  }
}
